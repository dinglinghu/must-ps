{
  "base_path": "reports/gantt",
  "auto_save": true,
  "create_backup": true,
  "default_formats": [
    "png",
    "svg",
    "json"
  ],
  "image_quality": "high",
  "image_dpi": 300,
  "image_width": 1600,
  "image_height": 1000,
  "max_files_per_type": 100,
  "auto_cleanup_days": 30,
  "archive_old_files": true,
  "compress_archives": true,
  "filename_template": "{chart_type}_{timestamp}_{mission_id}",
  "timestamp_format": "%Y%m%d_%H%M%S",
  "include_metadata": true,
  "async_save": true,
  "batch_save": true,
  "max_concurrent_saves": 3
}